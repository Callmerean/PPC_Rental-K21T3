
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    @if (Session["UserID"] != null)
    {
        if (int.Parse(Session["Role"].ToString()) == 0)
        {
            <title>PPCRENTAL - ADMIN</title>
        }
        else
        {
            <title>PPCRENTAL - AGENCY</title>
        }
    }
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="robots" content="all,follow">
    <!-- Bootstrap CSS-->
    <link rel="stylesheet" href="/Content/Mate/vendor/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome CSS-->
    <link rel="stylesheet" href="/Content/Mate/vendor/font-awesome/css/font-awesome.min.css">
    <!-- Fontastic Custom icon font-->
    <link rel="stylesheet" href="/Content/Mate/css/fontastic.css">
    <!-- Google fonts - Poppins -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,700">
    <!-- theme stylesheet-->
    <link rel="stylesheet" href="~/Content/Mate/css/style.red.css" id="theme-stylesheet">
    <!-- Custom stylesheet - for your changes-->
    <link rel="stylesheet" href="/Content/Mate/css/custom.css">
    <!-- Favicon-->
    <link rel="shortcut icon" href="/Content/Mate/img/favicon.ico">
    <!-- Tweaks for older IEs-->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
<body>
    <div class="page">
        <!-- Main Navbar-->
        <header class="header">
            <nav class="navbar">
                <!-- Search Box-->
                <div class="search-box">
                    <button class="dismiss"><i class="icon-close"></i></button>
                    <form id="searchForm" action="#" role="search">
                        <input type="search" placeholder="What are you looking for..." class="form-control">
                    </form>
                </div>
                <div class="container-fluid">
                    <div class="navbar-holder d-flex align-items-center justify-content-between">
                        <!-- Navbar Header-->
                        <div class="navbar-header">
                            <!-- Navbar Brand --><a href="~/Home/Index" class="navbar-brand">
                                <div class="brand-text brand-big"><span>PPC </span><strong> Rental</strong></div>
                                <div class="brand-text brand-small"><strong>PPC</strong></div>
                            </a>
                        </div>
                        <!-- Navbar Menu -->
                        <ul class="nav-menu list-unstyled d-flex flex-md-row align-items-md-center">
                            <!-- Search-->
                            <li class="nav-item d-flex align-items-center"><a id="search" href="#"><i class="icon-search"></i></a></li>
                            <!-- Logout    -->
                            <li class="nav-item"><a href="~/Client/Logout/@Session["UserID"]" class="nav-link logout">Logout<i class="fa fa-sign-out"></i></a></li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        <div class="page-content d-flex align-items-stretch">
            <!-- Side Navbar -->
            <nav class="side-navbar">
                <!-- Sidebar Header-->
                <div class="sidebar-header d-flex align-items-center">
                    <div class="avatar"><img src="/Content/Mate/img/avatar-1.jpg" alt="..." class="img-fluid rounded-circle"></div>
                    <div class="title">
                        @if (Session["UserID"] == null)
                        {
                            @Html.Partial("_LoginPartial")
                        }
                        else
                        {
                            <h1 class="h4">@Session["FullName"]</h1>
                        }
                        @if (Session["UserID"] != null)
                        {
                            if (int.Parse(Session["Role"].ToString())== 0)
                            {
                                <p>Sale</p>
                            }
                            else
                            {
                                <p>Agency</p>
                            }
                        }
                    </div>
                </div>
                <!-- Sidebar Navidation Menus--><span class="heading">Main</span>
                <ul class="list-unstyled">
                    @if (Session["UserID"] != null)
                    {
                        if (int.Parse(Session["Role"].ToString()) == 0)
                        {
                            <li class="active"><a href="~/Admin/AdminProperty/Index"> <i class="icon-grid"></i>Projects </a></li>
                        }
                        else
                        {
                            <li class="active"><a href="~/Client/Index"> <i class="icon-grid"></i>Projects </a></li>
                        }
                    }
                    <li><a href="~/Home/Index"> <i class="icon-home"></i>Home </a></li>
                    <li><a href="~/Client/ChangePassword"> <i class="icon-padnote"></i>Change Password </a></li>
                    <li><a href="login.html"> <i class="icon-interface-windows"></i>Login page </a></li>
                </ul>
            </nav>
            <div class="content-inner">
                <!-- Page Header-->
                <header class="page-header">
                    <div class="container-fluid">
                        <h2 class="no-margin-bottom">Real Estate</h2>
                    </div>
                </header>
                @RenderBody()
                <!-- Page Footer-->
                <footer class="main-footer">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-sm-6">
                                <p>PPC Rental &copy; 2017-2019</p>
                            </div>
                            <div class="col-sm-6 text-right">
                                <p>Design by <a href="https://bootstrapious.com/admin-templates" class="external">Rean</a></p>
                                <!-- Please do not remove the backlink to us unless you support further theme's development at https://bootstrapious.com/donate. It is part of the license conditions. Thank you for understanding :)-->
                            </div>
                        </div>
                    </div>
                </footer>
            </div>
        </div>
    </div>
    <!-- Javascript files-->
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="/Content/Mate/vendor/popper.js/umd/popper.min.js"> </script>
    <script src="/Content/Mate/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="/Content/Mate/vendor/jquery.cookie/jquery.cookie.js"> </script>
    <script src="/Content/Mate/vendor/chart.js/Chart.min.js"></script>
    <script src="/Content/Mate/vendor/jquery-validation/jquery.validate.min.js"></script>
    <script src="/Content/Mate/js/charts-home.js"></script>
    <!-- Main File-->
    <script src="js/front.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>