<head>

    <title>PPC - The Real Estate</title>

    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />

    <script src="js/pace.js"></script>

    <link rel="stylesheet" href="/Content/Hata/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/flexslider.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/jquery.selectBox.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/jquery-ui.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/green.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/style.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/responsive.css" type="text/css">
    <link rel="stylesheet" href="/Content/Hata/css/pace-minimal.css">


    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Ubuntu:400,700,500' rel='stylesheet' type='text/css'>

    <link rel="shortcut icon" href="favicon.ico">

    <!--[if lt IE 9]>
        <script src="js/html5shiv.js"></script>
    <![endif]-->

</head>

<body>

    <!-- TOP -->
    <div class="top-bar">

        <p class="text">
            <a><i class="fa fa-map-marker"></i> 45 Nguyen Khac Nhu Street, HCMC, Vietnam</a>
            <a href="tel:+61383766284"><i class="fa fa-phone"></i> + 84 9333 100 77</a>
        </p>

        <p class="social">
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-facebook"></i></a>
            <a href="#"><i class="fa fa-vimeo"></i></a>
            <a href="#"><i class="fa fa-instagram"></i></a>
            <a href="#"><i class="fa fa-skype"></i></a>
        </p>

    </div>
    <!-- TOP -->
    <!-- HEADER -->
    <header class="clearfix">

        <div class="logo">
            <a href="~/Home/Index"><img src="/Content/Hata/images/logo.png" alt="HATA">PPC RENTAL</a>
        </div>

        <nav class="navbar collapse" id="mobile-menu">
            <ul class="nav navbar-nav">
                <li class="active"><a href="~/Home/Index">Home</a></li>
                <li><a href="~/Home/About">About Us</a></li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown">Properties</a>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown">Services</a>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown">Page</a>
                    <ul class="dropdown-menu">
                    </ul>
                </li>
                <li><a href="~/News/News">News</a></li>
                <li><a href="contact.html">Contact Us</a></li>
            </ul>
        </nav>
    
        <div class="user">
            @if (Session["UserID"] == null)
            {
            <a href="~/Client/Login">
                <input id="Log" type="button" class="btn" value="Sign In" name="Login">
            </a>
            }
            else
            {
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                <img src="/Content/Hata/images/avatar1.png" alt="User Photo">
                <label>Welcome @Session["FullName"]</label>
                <span class="drop-arow"></span>
            </a>
            <ul class="dropdown-menu user-drop">
                <li><a href="#"><i class="fa fa-user"></i>My Profile</a></li>
                <li>
                @if (int.Parse(Session["Role"].ToString()) == 0)
                {
                    <a href="~/Admin/AdminProperty/Index"><i class="fa fa-list"></i>My Properties</a>
                }
                else
                {                  
                    <a href="~/Client/Index"><i class="fa fa-list"></i>My Properties</a>
                }

              </li>
                <li><a href="~/Client/Logout/@Session["UserID"]"><i class="fa fa-sign-out"></i>Logout</a></li>
            </ul>
            }
        </div>
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#mobile-menu" aria-expanded="false">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>

    </header>
    <!-- HEADER -->
    @RenderBody();
    <!-- FOOTER -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-5 copy-right">
                    <p><img src="/Content/Hata/images/footer-logo.png" alt="Logo" class="img-responsive"> &copy; 2015 HATA — Real Estate Site Template</p>
                </div>
                <div class="col-md-7">
                    <ul>
                        <li><a href="#">Home</a></li>
                        <li><a href="#">Properties</a></li>
                        <li><a href="#">Services</a></li>
                        <li><a href="#">Team</a></li>
                        <li><a href="#">Contact Us</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <!-- FOOTER -->

    <script src="/Content/Hata/js/jquery-1.11.3.min.js"></script>
    <script src="/Content/Hata/js/jquery-ui.js"></script>
    <script src="/Content/Hata/js/bootstrap.min.js"></script>
    <script src="/Content/Hata/js/masonry.pkgd.min.js"></script>
    <script src="/Content/Hata/js/jquery.flexslider-min.js"></script>
    @*<script src="/Content/Hata/js/jquery.selectBox.min.js"></script>*@
    <script src="/Content/Hata/js/icheck.js"></script>
    <script src="/Content/Hata/js/counter.animation.js"></script>
    <script src="/Content/Hata/js/jquery.appear.js"></script>
    <script src="/Content/Hata/js/custom.js"></script>
    <script src="/Content/Hata/js/jquery.cycle2.js"></script>

</body>

